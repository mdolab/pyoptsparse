[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[run]
omit =
    */build/*
    */doc/*
    */examples/*
    */postprocessing/*
    */test/*
    */setup.py
    pyoptsparse/pyALPSO/alpso_ext.py
    pyoptsparse/pyNOMAD/pyNOMAD.py
    pyoptsparse/pyNLPY_AUGLAG/pyNLPY_AUGLAG.py
    pyoptsparse/pyFSQP/pyFSQP.py
