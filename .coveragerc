[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[run]
omit =
    */build/*
    */doc/*
    */examples/*
    */postprocessing/*
    */test/*
    */setup.py
    */pyALPSO/alpso.py
    */pyALPSO/alpso_ext.py
